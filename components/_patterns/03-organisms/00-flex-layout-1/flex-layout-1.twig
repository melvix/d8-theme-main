{% set flex_layout_1_base_class = flex_layout_1_base_class|default('flex-layout-1') %}

{% set flex_grid_content_temp %}
  {% for item in flex_layout_1_items %}
    {% set temp_item_block %}
      {%- include "@molecules/05-test/test.twig" with {
        "test_items": item,
      } -%}
    {% endset %}
    {% include "@base/layouts/grid-item/_grid-item.twig" with {
      "grid_item_blockname": flex_layout_1_base_class,
      "grid_item_content": temp_item_block,
    } %}
  {% endfor %}
{% endset %}

{% set flex_grid_content = flex_grid_content|default(flex_grid_content_temp) %}

{% embed "@base/layouts/flex-grid/00-flex-grid.twig" with {
  "flex_grid_modifiers": flex_layout_1_modifiers,
  "flex_grid_blockname": flex_layout_1_base_class,
  "flex_grid_content": flex_grid_content,
}%}

{% block flex_layout_1_block %}
  {{ flex_layout_1_content }}
{% endblock flex_layout_1_block %}

{% endembed %}
