{% extends "paragraph.html.twig" %}

{% block paragraph %}

{% set tempArray1 = [] %}
{% set tempArray2 = [] %}
{% set tempArray3 = [] %}
{% set tempArray4 = [] %}
{% set tempArray5 = [] %}

{#{ kint(content.field_txex_taxo_ref_for_display) }#}
{#{ kint(content.field_txex_taxo_ref_for_display[0]['#taxonomy_term']) }#}
{#{ kint(content.field_txex_taxo_ref_for_display[0]['#taxonomy_term'].field_txex_ref_media_01) }#}

{#{ kint(content.field_txex_taxo_ref_for_display.entity) }#}
{#{ kint(content.field_txex_taxo_ref_for_display.0.entity) }#}
{#{ kint(content.field_txex_taxo_ref_for_display[0]['#taxonomy_term'].entity) }#}
{#{ kint(content.field_txex_taxo_ref_for_display[0]['#taxonomy_term'].field_txex_ref_media_01.entity) }#}
