{% extends "paragraph.html.twig" %}

  {% block content %}

    {% set tempArray1 = [] %}
    {% set tempArray2 = [] %}

    {% for key, child in content|without('field_txex_p01_image','field_variant') if key|first != '#' %}
      {% set tempArray1 = tempArray1|merge({(key) : (child),}) %}
    {% endfor %}

    {% for key, child in content.field_txex_p01_image if key|first != '#' %}
      {% set tempArray2 = tempArray2|merge({(key) : (child),}) %}
    {% endfor %}

    {% include "@molecules/05-test/test.twig" with {
      "test_modifiers": {"1": content.field_variant[0]['#markup']|clean_class},
      "test_blockname": "paragraph-01",
      "flex_grid_item": "",
      "test_items_1": tempArray1,
      "test_items_2": tempArray2,
    }%}

  {% endblock %}
