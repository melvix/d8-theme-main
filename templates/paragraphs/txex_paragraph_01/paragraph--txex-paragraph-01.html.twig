{% extends "paragraph.html.twig" %}

{% block content %}

{% set tempArray1 = [] %}
{% set tempArray2 = [] %}
{% set tempArray3 = [] %}
{% set tempArray4 = [] %}
{% set tempArray5 = [] %}

{% for key, child in content.field_txex_p01_image if key|first != '#' %}
  {% set tempArray1 = tempArray1|merge({(key) : (child),}) %}
{% endfor %}

{% for key, child in content|without('field_txex_p01_image','field_variant') if key|first != '#' %}
  {% set tempArray2 = tempArray2|merge({(key) : (child),}) %}
{% endfor %}

{% set tempArray3 = tempArray3|merge({0 : (tempArray1),}) %}
{% set tempArray3 = tempArray3|merge({1 : (tempArray2),}) %}

{% for key, child in content.field_variant if key|first != '#' %}
  {% set tempArray4 = tempArray4|merge({(key) : (child),}) %}
{% endfor %}

{% for key, child in tempArray4 %}
  {% set tempArray5 = tempArray5|merge({(key) : (child['#markup']),}) %}
{% endfor %}
{% set tempArray5 = tempArray5|default({1 : 'horizontal', 2 : 'feature-100' }) %}
{{ kint(tempArray5) }}

  {% include "@molecules/00-card/card.twig" with {
    "card_modifiers": tempArray5,
    "card_sections": tempArray3,
    "flexbox_section_modifiers": {1 : 'vertical', 2 : 'pl'},
    "flexbox_section_flexbox_extra": tempArray5,
  }%}


{% endblock content %}
